{
    "name": "cursor-charcode",
    "displayName": "Here Unicode Mate",
    "description": "Shows the Unicode code point of character under cursor in the status bar. And more encoding information.",
    "version": "0.9.1",
    "publisher": "buttaiwan",
    "engines": {
        "vscode": "^1.35.0"
    },
    "categories": [
        "Other"
    ],
    "galleryBanner": {
        "color": "#33A4C4",
        "theme": "dark"
    },
    "keywords": [
        "Unicode",
        "UTF-8",
        "UTF-16",
        "Glyphs",
        "encoding",
        "charset",
        "CJK",
        "font",
        "character",
        "emoji",
        "cursor"
    ],
    "icon": "resource/icon.png",
    "repository": {
        "type": "git",
        "url": "https://github.com/ButTaiwan/cursorCharCode"
    },
    "license": "SEE LICENSE IN LICENSE.txt",
    "bugs": {
        "url": "https://github.com/ButTaiwan/cursorCharCode/issues"
    },
    "activationEvents": [
        "*"
    ],
    "main": "./out/extension",
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.5.3",
        "@types/vscode": "^1.35.0",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    },
    "contributes": {
        "keybindings":[
            {
                "command": "cursorCharcode.queryUnicodeChar",
                "key": "ctrl+i",
                "mac": "cmd+i"
            }
        ],
        "commands": []
    }
}
