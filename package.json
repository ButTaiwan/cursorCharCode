{
    "name": "cursorCharCode",
    "displayName": "cursorCharCode",
    "description": "Shows the Unicode value of character under cursor in the status bar.",
    "version": "0.0.1",
    "publisher": "zeithaste",
    "engines": {
        "vscode": "^1.17.0"
    },
    "categories": [
        "Other"
    ],
    "galleryBanner": {
        "color": "#33A4C4",
        "theme": "dark"
    },
    "activationEvents": [
        "*"
    ],
    "main": "./out/extension",
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.5.3",
        "vscode": "^1.1.5",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    }
}